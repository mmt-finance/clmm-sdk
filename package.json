{
  "name": "@mmt-finance/clmm-sdk",
  "version": "1.1.4",
  "description": "Momentum SDK to access the Momentum CLMM",
  "private": false,
  "author": "Momentum Finance",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@commitlint/config-conventional": "^19.7.1",
    "@mysten/sui": "1.28.2",
    "@types/bn.js": "^5.1.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.14.11",
    "bn.js": "^5.2.1",
    "branch-name-lint": "^2.1.1",
    "commitlint": "^19.7.1",
    "decimal.js": "^10.5.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "scripts": {
    "prepare": "husky",
    "build": "rm -rf dist && tsc --project tsconfig.build.json",
    "test": "jest",
    "prettier:fix": "npx prettier --write '**/*.{js,ts,json}' --log-level error",
    "release": "yarn build && yarn publish",
    "scripts": "ts-node"
  },
  "peerDependencies": {
    "@mysten/sui": "^1.3.0",
    "axios": "^1.4.0",
    "bn.js": "^5.2.1",
